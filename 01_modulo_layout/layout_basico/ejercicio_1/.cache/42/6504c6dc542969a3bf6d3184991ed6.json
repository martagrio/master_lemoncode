ce-evenly;\r\n  flex-wrap: wrap;\r\n  justify-items: flex-start;\r\n}\r\n\r\n@mixin box-style {\r\n  border: 1px solid transparent;\r\n  border-radius: 5px;\r\n  padding: 30px 5px;\r\n\r\n  .color-box-text {\r\n    color: #818886;\r\n  }\r\n\r\n  &.base-color {\r\n    padding: 30px 15px;\r\n    span {\r\n      color: white;\r\n    }\r\n  }\r\n}\r\n\r\n@function ligthenColor($base-color, $level) {\r\n  @return lighten($base-color, 5 * $level);\r\n}\r\n\r\n@function darkenColor($base-color, $level) {\r\n  @return darken($base-color, 5 * $level);\r\n}\r\n\r\nbody {\r\n  display: grid;\r\n  grid-template-rows: 1fr 1fr 1fr;\r\n  grid-row-gap: 20px;\r\n}\r\n\r\n.container-1,\r\n.container-2,\r\n.container-3 {\r\n  @include container-style();\r\n}\r\n\r\n.box {\r\n  @include box-style();\r\n}\r\n\r\n$styles: (\r\n  \"container-1\": $base-color-1, \r\n  \"container-2\": $base-color-2, \r\n  \"container-3\": $base-color-3\r\n);\r\n\r\n@each $container, $color in $styles {\r\n  .#{$container} {\r\n    .box {\r\n      background-color: $color;\r\n    }\r\n\r\n    .color-box-text {\r\n      color: complement($color);\r\n    }\r\n\r\n    @for $i from 1 through 4 {\r\n      .lighten-#{$i} {\r\n        background-color: ligthenColor($color, $i);\r\n      }\r\n\r\n      .darken-#{$i} {\r\n        background-color: darkenColor($color, $i);\r\n      }\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"1f258066d32f93769263a064db5d2b06","cacheData":{"env":{}}}